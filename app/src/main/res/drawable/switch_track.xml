<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="100" android:exitFadeDuration="140">
    <item android:state_checked="true">
        <shape android:shape="rectangle" android:tint="#5FFFFFFF" android:tintMode="multiply" android:useLevel="false" android:visible="true">
            <solid android:color="?colorPrimary" />
            <corners android:radius="30dp" />
            <size android:width="30dp" android:height="20dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/switch_track" />
            <corners android:radius="30dp" />
            <size android:width="30dp" android:height="20dp" />
        </shape>
    </item>
</selector>